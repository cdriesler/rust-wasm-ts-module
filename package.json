{
    "name": "rust-wasm-ts-module",
    "version": "1.0.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "npm run build:wasm && npm run build:ts",
        "build:ts": "tsc",
        "build:wasm": "wasm-pack build --target bundler --out-dir src/wasm",
        "postbuild": "cpy src/wasm/*.wasm dist/wasm"
    },
    "devDependencies": {
        "cpy-cli": "^3.0.0",
        "typescript": "^3.7.4"
    }
}
